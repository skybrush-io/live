{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions"],
          "node": "current"
        },
        "useBuiltIns": "usage",
        "corejs": "3.18"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", {}],
    [
      "i18next-extract",
      {
        "discardOldKeys": true,
        "locales": ["en", "hu"],
        "outputPath": "src/i18n/{{locale}}.json"
      }
    ]
  ]
}
